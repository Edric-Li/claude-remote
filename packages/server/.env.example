# Database Configuration
DB_TYPE=postgres
DB_HOST=your_db_host
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
DB_SSL=false
DB_LOGGING=false

# Security Configuration
# 用于加密仓库凭据的密钥，建议使用至少32个字符的随机字符串
ENCRYPTION_KEY=your_secret_encryption_key_here_minimum_32_chars

# Node Environment
NODE_ENV=development