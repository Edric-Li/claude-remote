{
  "name": "ai-orchestra",
  "version": "0.1.0",
  "description": "Orchestrate Your AI Workers - Distributed AI Coding Platform",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:server": "pnpm --filter @ai-orchestra/server dev",
    "dev:web": "pnpm --filter @ai-orchestra/web dev",
    "dev": "concurrently -n server,web \"pnpm run dev:server\" \"pnpm run dev:web\"",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3"
  }
}